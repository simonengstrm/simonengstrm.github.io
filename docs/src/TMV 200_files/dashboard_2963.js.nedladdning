  onUploadComplete: function() {
    if (!P.feed.uploading) return false;
    P.feed.uploading = false;
    $.unblockUI();
    if (P.feed.userProfile && P.feed.userProfile.profile_settings && P.feed.userProfile.profile_settings.published) {
