        $('#folder_must_reads_count').text(P.folders_bar.must_read_new);
        $('#folder_must_reads_count').addClass("show");
      } else {
        $('#folder_must_reads_count').removeClass("show");
      }
    } else {
      $('#must_read_folder').hide();
    }
