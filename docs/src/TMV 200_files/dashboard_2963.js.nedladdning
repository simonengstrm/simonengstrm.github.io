      for (var i = 0; i < P.old_new_post.user.config.feedback[net.id].length; i++) {
        var f = P.old_new_post.user.config.feedback[net.id][i];
        if (f.cnt > 0) {
          if (!feedback) feedback = [];
          feedback.push(f);
          total += f.cnt;
        }
      }
      if (total < 3) {
        feedback = false;
      }
      if (total > 9) feedbackClass = "medium";
