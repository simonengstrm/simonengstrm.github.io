    data.notMe = !data.me;
    data.asCompany = asCompany;
    var showPrivacyMessage = false;
    var forStudent = true;
    if (forCompany) forStudent = false;
    var canAdmin = false;
    if (typeof(CAN_ADMIN) != 'undefined' && CAN_ADMIN) canAdmin = true;

    P.user_careers_profile.prepareProfile(data);
