    }
    else {
      P.modules.load("/modules/shared_templates/" + name + ".html", 1, function(html) {
        PTM.templates[name] = html;
        if (callback)
          callback.call(window, PTM.templates[name], options);
      }, function(err) {alert(err)});
    }
