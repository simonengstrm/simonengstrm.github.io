        confirm: "Yes",
        cancel: "No",
        callback: function() {
          PA.call_pj("content.confirm_read", mr, 1);
          if (okCallback) okCallback.call(window);
        },
        cancelCallback: okCallback,
      };
      PEM.fire("showConfirmationWindow", params);
    } else {
      if (okCallback) okCallback.call(window);
    }
  },
